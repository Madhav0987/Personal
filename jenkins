pipeline{
  agent any
    tools {
	  jdk 'jdk17'
	  maven 'Maven3'
	}
Stages{
  stage("git checkout"){
    steps{
      git branch: 'main', changelog: false, poll: false, url: 'https://github.com/Madhav0987/Personal.git'
    }
  }
  stage("builing image"){
      steps{
        script{
          withDockerRegistry(credentialsId: '42a3b536-bbc2-445d-be92-6ba6d4ababb8', toolName: 'docker') {
              sh 'ocker build -t ubuntu:v1 .'    
}
}
      }
  }

}
}
